<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ValidatorEditText">
        <attr name="errorMessage" format="string"/>
        <attr name="checkType" format="integer">
            <enum name="check_type:alpha" value="0"/>
            <enum name="check_type:numeric" value="1"/>
            <enum name="check_type:dob" value="2"/>
            <enum name="check_type:phone_number" value="3"/>
            <enum name="check_type:email" value="4"/>
            <enum name="check_type:alphanumeric" value="5"/>
            <enum name="check_type:username" value="6"/>
            <enum name="check_type:password" value="7"/>
        </attr>
        <attr name="validBackground" format="reference"/>
        <attr name="invalidBackground" format="reference"/>
        <attr name="useRegex" format="string"/>
        <attr name="timeOut" format="integer"/>
        <attr name="compareTo" format="reference"/>
    </declare-styleable>
</resources>